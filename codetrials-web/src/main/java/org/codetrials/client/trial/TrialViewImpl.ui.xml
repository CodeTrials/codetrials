<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:HTMLPanel addStyleNames="row">
        <div class="col-md-4">
            <div class="panel panel-primary">
                <div class="panel-body">
                    <div class="progress">
                        <div class="progress-bar" ui:field="progress"> </div>
                    </div>
                    <h3 ui:field="title"> </h3>
                    <div ui:field="description"> </div>
                </div>
            </div>
        </div>
        <div class="col-md-8" ui:field="consoleContainer">
        </div>
    </g:HTMLPanel>
</ui:UiBinder>